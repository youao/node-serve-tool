{
  "name": "拉取百应榜单",
  "version": "1.0",
  "description": "拉取百应榜单",
  "manifest_version": 3,
  "permissions": [
    "downloads",
    "storage",
    "cookies",
    "tabs"
  ],
  "host_permissions": ["https://buyin.jinritemai.com/*"],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "images/icon-48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://buyin.jinritemai.com/*"
      ],
      "css": [],
      "js": ["js/content-script.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "background": {
    "scripts": ["js/backgound.js"],
    "service_worker": "js/service.js",
    "presistent": true
  },
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "page_action": {
    "default_icon": {
      "16": "images/icon-16.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  }
}